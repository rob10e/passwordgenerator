{
  "name": "passwordgenerator",
  "productName": "Password Generator",
  "version": "1.0.0-beta.2",
  "description": "A cross-platform desktop password generation collection",
  "main": "src/index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development electron-forge start",
    "package": "cross-env NODE_ENV=production electron-forge package",
    "make": "cross-env NODE_ENV=production electron-forge make",
    "lint": "eslint src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rob10e/passwordgenerator.git"
  },
  "keywords": [],
  "author": {
    "name": "Robert Tenney",
    "url": "http://www.tenneysoftware.com",
    "email": "rtenney@tenneysoftware.com"
  },
  "license": "MIT",
  "babel": {
    "env": {
      "development": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": "1.7"
              }
            }
          ],
          "react"
        ],
        "plugins": [
          "react-hot-loader/babel",
          "transform-async-to-generator",
          "transform-class-properties",
          "transform-es2015-spread",
          "transform-object-rest-spread"
        ],
        "sourceMaps": "both"
      },
      "production": {
        "presets": [
          [
            "env",
            {
              "targets": {
                "electron": 1.7
              }
            }
          ],
          "react"
        ],
        "plugins": [
          "transform-async-to-generator",
          "transform-class-properties",
          "transform-es2015-spread",
          "transform-object-rest-spread"
        ],
        "sourceMaps": "none"
      }
    }
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": ["squirrel"],
        "darwin": ["zip"],
        "linux": ["deb"]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "passwordgenerator"
      },
      "electronInstallerDebian": {
        "icon": "resources/Icon.png",
        "categories": ["Utility"],
        "homepage": "http://www.tenneysoftware.com"
      },
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "rob10e",
        "name": "passwordgenerator"
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "passwordgenerator"
      },
      "publish_targets": {
        "win32": ["github"],
        "darwin": ["github"],
        "linux": ["github"]
      }
    }
  },
  "dependencies": {
    "@blueprintjs/core": "^1.33.0",
    "copy-paste": "^1.3.0",
    "electron-compile": "^6.4.2",
    "guid": "^0.0.12",
    "lodash": "^4.17.4",
    "node-clipboard": "^1.2.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-addons-css-transition-group": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-dom-factories": "^1.0.2",
    "react-height": "^3.0.0",
    "react-hot-loader": "^3.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-selectize": "^3.0.1",
    "react-transition-group": "^1.1.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.4.0",
    "redux-persist-electron-storage": "^1.1.2",
    "redux-thunk": "^2.2.0",
    "electron-devtools-installer": "^2.2.3"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.2",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-spread": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.1.1",
    "electron-forge": "^4.1.5",
    "electron-prebuilt-compile": "1.7.9",
    "electron-react-devtools": "^0.5.2",
    "eslint": "3",
    "eslint-config-airbnb": "15",
    "eslint-plugin-import": "2",
    "eslint-plugin-jsx-a11y": "5",
    "eslint-plugin-react": "7"
  }
}
